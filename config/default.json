  "Plugin": {
    "perfmon": {
      "counters" : [
          {
            "counter": "\\LogicalDisk(C:)\\% Free Space",
            "enable": 1,
            "plugin": "perfmon_LogicalDisk",
            "plugin_instance": "C",
            "type": "percent",
            "type_instance": "Free Space"
          }
      ]
    }
  }
}
